{
  "name": "multi",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:web": "pnpm --filter web dev",
    "dev:admin": "pnpm --filter admin dev",
    "dev:user-panel": "pnpm --filter userPanel dev",
    "dev:auth": "pnpm --filter auth dev",
    "dev": "concurrently -n web,admin,user-panel, -c blue,green,magenta,cyan \"pnpm dev:web\" \"pnpm dev:admin\" \"pnpm dev:user-panel\"",
    "build:web": "pnpm --filter web build",
    "build:admin": "pnpm --filter admin build",
    "build:user-panel": "pnpm --filter userPanel build",
    "build:auth": "pnpm --filter auth build",
    "build:all": "pnpm -r build",
    "start:web": "pnpm --filter web start -p 3000",
    "start:admin": "pnpm --filter admin start -p 3001",
    "start:user-panel": "pnpm --filter userPanel start -p 3002",
    "start:auth": "pnpm --filter auth start",
    "start:all": "pnpm -r start"
  },
  "keywords": [],
